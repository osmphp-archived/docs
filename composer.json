{
    "name": "osmphp/docs",
    "description": "Osm modules for displaying text documents",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "osmphp/data": "^0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Osm\\Docs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osm\\Docs\\Tests\\": "tests/",
            "Osm\\Docs\\Samples\\": "samples/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/osmphp/core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osmphp/framework"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osmphp/data"
        }
    ]
}
