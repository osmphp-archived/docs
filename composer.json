{
    "name": "osmphp/docs",
    "autoload": {
        "psr-4": {
            "Osm\\Docs\\": "src/",
            "Osm\\Docs\\Tests\\": "tests/",
            "Osm\\Docs\\Samples\\": "samples/"
        }
    },
    "require": {
        "php": "^7.2",
        "osmphp/framework": "2.*",
        "michelf/php-markdown": "1.8.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "osm": {
            "component_pools": {
                "src": {
                    "module_path": "*/Module.php",
                    "theme_path": "*/theme.php"
                },
                "samples": {
                    "module_path": "*/Module.php",
                    "testing": true
                }
            }
        }
    }
}
